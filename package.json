{
  "name": "koios-tiny-client",
  "version": "1.0.10",
  "description": "TypeScript client fot KOIOS Elastic Cardano Query Layer",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "prepare": "curl \"https://api.koios.rest/koiosapi.yaml\" > codegen/koiosapi.yaml && node codegen/_prepare.js",
    "codegen": "node codegen/_generator.js",
    "format": "prettier --write \"{codegen,src}/**/*.{js,jsx,ts,tsx,json,md,scss,css}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ray Network",
  "license": "MIT",
  "devDependencies": {
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "yaml": "^2.2.2"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dereference-json-schema": "^0.2.1"
  }
}
